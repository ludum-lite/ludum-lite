{
  "name": "server",
  "private": true,
  "scripts": {
    "dev": "webpack --config config/webpack.development.js",
    "build": "webpack --config config/webpack.production.js",
    "start": "node dist/server",
    "start:env": "node --require dotenv/config dist/server dotenv_config_path=config/.env"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.8.1",
    "apollo-server": "^2.12.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/webpack-env": "^1.15.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "prettier": "2.0.4",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/typescript"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/proposal-class-properties"
    ]
  }
}
