{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently -n CLIENT,CODEGEN \"env-cmd -e development react-scripts start\" \"yarn codegen\"",
    "start:dev:prod": "concurrently -n CLIENT,CODEGEN \"env-cmd -e developmentProduction react-scripts start\" \"yarn codegen\"",
    "start:prod": "concurrently -n CLIENT,CODEGEN \"env-cmd -e production react-scripts start\" \"yarn codegen\"",
    "codegen": "graphql-codegen --watch --config codegen.yml",
    "build": "env-cmd -e production react-scripts build",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
